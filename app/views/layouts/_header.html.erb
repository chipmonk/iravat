<header>
  <%= link_to logo, root_path %>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Profile", current_user %></li>
      <% if user_signed_in? %>
      <li><%= link_to "Sign out", destroy_user_session_path(current_user), :method => :delete %></li>
      <% else %>
      <li><%= link_to "Sign in", new_user_session_path %></li>
      <% end %>
    </ul>
  </nav>
</header>

